            </tr>
            <tr>
                <td align="right" style="height: 16px">
                    <asp:LinkButton ID="lnkbtnLogout" runat="server" Font-Bold="True" Font-Names="Arial" Font-Size="9pt" Font-Underline="False" ForeColor="#0000C0" OnClick="lnkbtnLogout_Click">Logout</asp:LinkButton>
                </td>
            </tr>
            <tr>
                <td valign="top" height="470">
                    <br />
                    <br />
                    <br />
                    <asp:ContentPlaceHolder ID="ContentPlaceHolder1" runat="server">
                    </asp:ContentPlaceHolder>
